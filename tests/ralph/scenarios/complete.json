{
  "name": "complete",
  "description": "Always outputs RALPH_COMPLETE - used to test successful task completion",
  "phases": {
    "plan": {
      "messages": {
        "work": "Creating spec...",
        "done": "Spec created successfully."
      },
      "signal": "RALPH_COMPLETE"
    },
    "ready": {
      "messages": {
        "work": "Creating task breakdown...",
        "done": "Tasks created successfully."
      },
      "signal": "RALPH_COMPLETE"
    },
    "step": {
      "messages": {
        "work": "Implementing task...",
        "done": "Implementation complete.\nAll quality gates passed."
      },
      "signal": "RALPH_COMPLETE"
    }
  }
}
